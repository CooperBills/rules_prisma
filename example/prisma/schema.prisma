generator client {
  provider = "prisma-client-js"
  output   = "./myclient" // output directory must match the prisma_js_library rule name
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
